# Backend Environment Variables
# Copy this file to .env and fill in your actual values
# ⚠️ NEVER commit the actual .env file to git!

# Supabase Project URL
SUPABASE_URL=https://your-project-ref.supabase.co/

# Supabase Service Role Key (admin privileges - KEEP SECRET!)
# ⚠️ This key bypasses all RLS policies - never expose in frontend
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Supabase Anonymous Key (for JWT verification in auth middleware)
SUPABASE_ANON_KEY=your-anon-key-here

# Supabase Storage URL (optional)
SUPABASE_STORAGE_URL=https://your-project-ref.storage.supabase.co/storage/v1/s3

# Server Port
PORT=5000
