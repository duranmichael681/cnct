# Frontend Environment Variables
# Copy this file to .env.local and fill in your actual values

# Supabase Project URL
VITE_SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase Anonymous/Public Key (safe to expose in frontend)
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Supabase Storage URL (optional, for direct storage access)
VITE_SUPABASE_STORAGE_URL=https://your-project-ref.storage.supabase.co/storage/v1/s3

# Backend API URL (where your Express server runs)
VITE_API_URL=http://localhost:5000/api

# Storage bucket name for post images
VITE_POSTS_BUCKET=posts_picture
